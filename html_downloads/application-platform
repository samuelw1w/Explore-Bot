<script>
  
  var numLocale = "en";
  
  const apps = document.querySelector('#apps');
  const output = document.querySelector('.apps-output');
  
  output.textContent = apps.value;
  
  apps.addEventListener('input', () => {
    output.textContent = apps.value;
  });
  
  const months = document.querySelector('#months');
  const monthsOutput = document.querySelector('.months-output');
  
  monthsOutput.textContent = months.value;
  
  months.addEventListener('input', () => {
    monthsOutput.textContent = months.value;
  });
  
  const revenue = document.querySelector('#revenue');
  const revenueOutput = document.querySelector('.revenue-output');
  
  revenueOutput.textContent = parseInt(revenue.value).toLocaleString(numLocale);
  
  revenue.addEventListener('input', () => {
    revenueOutput.textContent = parseInt(revenue.value).toLocaleString(numLocale);
  });
  
  
  var calculation = (parseInt(apps.value) * 9.6 * parseInt(revenue.value)) - (parseInt(apps.value) * (12-parseInt(months.value)) * parseInt(revenue.value));
  
  
  $("#result").text(calculation.toLocaleString(numLocale));
  
  const result = document.querySelector('#result');
  
  
  document.querySelectorAll('.input').forEach(item => {
  item.addEventListener('input', () => {
  	result.textContent = ((parseInt(apps.value) * 9.6 * parseInt(revenue.value)) - (parseInt(apps.value) * (12-parseInt(months.value)) * parseInt(revenue.value))).toLocaleString(numLocale);
  });
  });
  
  if (location.href.indexOf('calculator') > -1) {
  page = " (Standalone)"
  } else {
  page = ""
  }
  
  $(apps).click(function(){
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({
  		'event' : 'tvcEvent',  
  		'eventCategory': 'TAP revenue calculator' + page,  
  		'eventAction': 'Clicked on sliders', 
  		'eventLabel': 'Clicked apps slider'
  });
  });
  $(months).click(function(){
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({
  		'event' : 'tvcEvent',  
  		'eventCategory': 'TAP revenue calculator' + page,  
  		'eventAction': 'Clicked on sliders', 
  		'eventLabel': 'Clicked months slider'
  });
  });
  $(revenue).click(function(){
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({
  		'event' : 'tvcEvent',  
  		'eventCategory': 'TAP revenue calculator' + page,  
  		'eventAction': 'Clicked on sliders', 
  		'eventLabel': 'Clicked revenue slider'
  });
  });
</script>

