  window.addEventListener('load', function(){
  	let checkCounter = 0;
  	var checkForItems = function(){
  		let uberitems = document.querySelectorAll('#collection-items li a.item-link');
  		if(uberitems.length > 0){
  			uberitems.forEach(function(item){
  				let title = item.getAttribute("data-page-title");
  				item.setAttribute("aria-label", title);
  			});
  		}else{
  			if(checkCounter < 7){
  				checkCounter += 1;
  				setTimeout(function(){
  					checkForItems();
  				},1000);
  			}
  		}
  	};
  
  	setTimeout(function(){
  		checkForItems();
  	},500);
  });
</script>
